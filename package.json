{
	"name": "@skyra/saelem",
	"version": "2.0.4",
	"description": "GraphQL Horoscope API for Skyra",
	"author": "@skyra",
	"license": "MIT",
	"types": "generated/ts/saelem.d.ts",
	"main": "generated/ts/saelem.d.ts",
	"homepage": "https://www.github.com/skyra-project/saelem",
	"scripts": {
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write --loglevel=warn src/**/*.{ts,json}",
		"test": "jest",
		"start": "node generated/api",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"dev": "yarn build && yarn start",
		"commit": "git-cz",
		"cz": "git-cz",
		"update": "yarn upgrade-interactive --latest",
		"sversion": "standard-version",
		"codegen": "graphql-codegen",
		"postcodegen": "run-s typescript:** graphql:**",
		"typescript:replace-invalid-code": "replace-in-file /}\\;/g } generated/ts/saelem.d.ts --isRegex",
		"graphql:replace-query": "replace-in-file \"type Query \" \"type SaelemQuery \" generated/ts/saelem.graphql"
	},
	"dependencies": {
		"graphql": "^15.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@favware/eslint-config": "^6.0.2",
		"@favware/ts-config": "^3.0.4",
		"@graphql-codegen/cli": "^1.19.0",
		"@graphql-codegen/schema-ast": "^1.18.0",
		"@graphql-codegen/typescript": "^1.17.11",
		"@graphql-codegen/typescript-resolvers": "^1.17.10",
		"@skyra/timestamp": "^1.0.0",
		"@types/cheerio": "^0.22.22",
		"@types/jest": "^26.0.15",
		"@types/koa": "^2.11.6",
		"@types/module-alias": "^2.0.0",
		"@types/node": "^14.14.6",
		"@types/node-fetch": "^2.5.7",
		"@types/redis": "^2.8.28",
		"@types/redis-mock": "^0.17.0",
		"@typescript-eslint/eslint-plugin": "^4.6.0",
		"@typescript-eslint/parser": "^4.6.0",
		"apollo-server-koa": "^2.19.0",
		"cheerio": "^1.0.0-rc.3",
		"class-validator": "^0.12.2",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.12.1",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"koa": "^2.13.0",
		"lint-staged": "^10.5.1",
		"module-alias": "^2.2.2",
		"node-fetch": "^2.6.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.1.0",
		"redis": "^3.0.2",
		"redis-mock": "^0.54.0",
		"reflect-metadata": "^0.1.13",
		"replace-in-file": "^6.1.0",
		"standard-version": "^9.0.0",
		"ts-jest": "^26.4.3",
		"ts-node": "^9.0.0",
		"type-graphql": "^1.1.1",
		"typescript": "^4.0.5",
		"utility-types": "^3.10.0"
	},
	"files": [
		"generated/ts/saelem.d.ts",
		"generated/ts/saelem.graphql"
	],
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/saelem.git"
	},
	"bugs": {
		"url": "https://github.com/skyra-project/saelem/issues"
	},
	"keywords": [
		"skyra",
		"skyra project",
		"horoscope",
		"api",
		"typescript",
		"ts",
		"yarn",
		"graphql"
	],
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn pretty-quick --staged && yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"acorn": "^8.0.4",
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"lodash": "^4.17.20",
		"dot-prop": "^6.0.0",
		"node-fetch": "^2.6.1"
	},
	"_moduleAliases": {
		"@redis": "generated/api/lib/redis",
		"@utils": "generated/api/lib/utils",
		"@lib": "generated/api/lib",
		"@root": "generated/api"
	}
}
