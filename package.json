{
	"name": "@skyra/saelem",
	"version": "3.0.5",
	"description": "GraphQL Horoscope API for Skyra",
	"author": "@skyra",
	"license": "MIT",
	"types": "generated/ts/saelem.d.ts",
	"main": "generated/ts/saelem.d.ts",
	"type": "module",
	"exports": {
		"import": "./generated/ts/graphql-pokemon.d.ts",
		"require": "./generated/ts/graphql-pokemon.d.ts"
	},
	"imports": {
		"#utils/*": "./generated/api/lib/utils/*.js",
		"#redis/*": "./generated/api/lib/redis/*.js",
		"#lib/*": "./generated/api/lib/*.js",
		"#root/*": "./generated/api/*.js"
	},
	"sideEffects": false,
	"homepage": "https://www.github.com/skyra-project/saelem",
	"scripts": {
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write --loglevel=warn src/**/*.{ts,json}",
		"test": "jest",
		"start": "node generated/api",
		"build": "tsc -b src",
		"clean": "node scripts/clean.mjs",
		"watch": "tsc -b src -w",
		"dev": "yarn build && yarn start",
		"commit": "git-cz",
		"cz": "git-cz",
		"update": "yarn upgrade-interactive --latest",
		"sversion": "standard-version",
		"codegen": "graphql-codegen",
		"postcodegen": "run-s typescript:** graphql:**",
		"typescript:replace-invalid-code": "replace-in-file /}\\;/g } generated/ts/saelem.d.ts --isRegex",
		"graphql:replace-query": "replace-in-file \"type Query \" \"type SaelemQuery \" generated/ts/saelem.graphql",
		"prepublishOnly": "yarn build",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"graphql": "^15.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.1.4",
		"@commitlint/config-conventional": "^12.1.4",
		"@graphql-codegen/cli": "^1.21.4",
		"@graphql-codegen/schema-ast": "^1.18.2",
		"@graphql-codegen/typescript": "^1.22.0",
		"@graphql-codegen/typescript-resolvers": "^1.19.1",
		"@sapphire/eslint-config": "3.1.3",
		"@sapphire/fetch": "^1.0.1",
		"@sapphire/prettier-config": "1.1.2",
		"@sapphire/time-utilities": "^1.3.1",
		"@sapphire/ts-config": "2.2.2",
		"@sapphire/utilities": "^1.5.1",
		"@types/cheerio": "^0.22.29",
		"@types/ioredis": "^4.26.4",
		"@types/jest": "^26.0.23",
		"@types/koa": "^2.13.1",
		"@types/node": "^15.6.1",
		"@types/node-fetch": "^2.5.10",
		"apollo-server-koa": "^2.25.0",
		"cheerio": "1.0.0-rc.9",
		"class-validator": "^0.13.1",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^6.0.0",
		"ioredis": "^4.27.3",
		"ioredis-mock": "^5.5.6",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"koa": "^2.13.1",
		"lint-staged": "^11.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.0",
		"pretty-quick": "^3.1.0",
		"reflect-metadata": "^0.1.13",
		"replace-in-file": "^6.2.0",
		"standard-version": "^9.3.0",
		"streamsearch": "^0.1.2",
		"ts-jest": "^26.5.6",
		"ts-node": "^9.1.1",
		"type-graphql": "^1.1.1",
		"typescript": "^4.2.4",
		"utility-types": "^3.10.0"
	},
	"files": [
		"generated/ts/saelem.d.ts",
		"generated/ts/saelem.graphql"
	],
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/saelem.git"
	},
	"bugs": {
		"url": "https://github.com/skyra-project/saelem/issues"
	},
	"keywords": [
		"skyra",
		"skyra project",
		"horoscope",
		"api",
		"typescript",
		"ts",
		"yarn",
		"graphql"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"acorn": "^8.2.4",
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"lodash": "^4.17.21",
		"dot-prop": "^6.0.1"
	},
	"prettier": "@sapphire/prettier-config"
}
