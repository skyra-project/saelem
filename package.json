{
  "name": "@skyra/saelem",
  "version": "1.0.2",
  "description": "GraphQL Horoscope API for Skyra",
  "author": "@skyra",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.d.ts",
  "homepage": "https://www.github.com/skyra-project/saelem",
  "scripts": {
    "lint": "eslint src scripts tests --ext ts --fix",
    "style": "prettier --write --loglevel=warn src/**/*.{ts,json}",
    "test": "jest",
    "start": "node generated/api",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "dev": "yarn build && yarn start",
    "commit": "git-cz",
    "cz": "git-cz",
    "up": "yarn upgrade-interactive --latest",
    "sversion": "standard-version",
    "codegen": "graphql-codegen",
    "cloud:start": "node index.js"
  },
  "dependencies": {
    "graphql": "^15.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@favware/eslint-config": "^5.1.0",
    "@favware/ts-config": "^3.0.2",
    "@graphql-codegen/cli": "^1.16.0",
    "@graphql-codegen/schema-ast": "^1.16.0",
    "@graphql-codegen/typescript": "^1.16.0",
    "@graphql-codegen/typescript-resolvers": "^1.16.0",
    "@klasa/console": "^0.0.3",
    "@klasa/timestamp": "^0.0.1",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "apollo-server-express": "^2.15.1",
    "cheerio": "^1.0.0-rc.3",
    "class-validator": "^0.12.2",
    "cz-conventional-changelog": "^3.2.0",
    "express": "^4.17.1",
    "graphql-parse-resolve-info": "^4.7.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "lint-staged": "^10.2.11",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.1.1",
    "type-graphql": "^1.0.0-rc.3",
    "typescript": "^3.9.6",
    "utility-types": "^3.10.0"
  },
  "files": [
    "generated/ts/saelem.d.ts",
    "generated/ts/saelem.graphql"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyra-project/saelem.git"
  },
  "bugs": {
    "url": "https://github.com/skyra-project/saelem/issues"
  },
  "keywords": [
    "skyra",
    "skyra project",
    "horoscope",
    "api",
    "typescript",
    "ts",
    "yarn",
    "graphql"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --ext ts",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "acorn": "^7.3.1",
    "minimist": "^1.2.5",
    "kind-of": "^6.0.3",
    "jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
    "jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz"
  }
}
