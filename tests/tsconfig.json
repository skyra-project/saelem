{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "ES2019",
		"noEmit": true,
		"incremental": false,
		"baseUrl": ".",
		"paths": {
			"@utils/*": ["../src/lib/utils/*"],
			"@redis/*": ["../src/lib/redis/*"],
			"@lib/*": ["../src/lib/*"],
			"@root/*": ["../src/*"]
		}
	},
	"include": ["./**/*.ts", "../src/**/*"],
	"references": [{ "path": "../src" }]
}
