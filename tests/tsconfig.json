{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"#utils/*": ["../src/lib/utils/*"],
			"#redis/*": ["../src/lib/redis/*"],
			"#lib/*": ["../src/lib/*"],
			"#root/*": ["../src/*"],
			"#tests/*": ["./*"]
		}
	},
	"include": [".", "../src"],
	"references": [{ "path": "../src" }]
}
